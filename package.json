{
  "name": "Poemist",
  "version": "1.0.0",
  "repository": "https://github.com/tracykm/poemist",
  "description": "A website for creating found poems, webpack config from https://github.com/ReactJSResources/react-webpack-babel",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --profile --colors",
    "test": "export NODE_ENV=test && jest",
    "test-watch": "NODE_PATH=./ jest --watch --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "NODE_ENV='production' webpack --config webpack.production.config.js --progress --profile --colors",
    "dev": "API_URL=http://localhost:3000/api npm start webpack-dashboard -- webpack-dev-server",
    "fakeserver": "json-server --watch .json-server/db.js --routes .json-server/routes.json",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "getSchema": "SCHEMA_FILE=cypress/schema.js && echo 'module.exports = `' > $SCHEMA_FILE && get-graphql-schema http://localhost:3000/api/graphql >> $SCHEMA_FILE && echo '`' >> $SCHEMA_FILE",
    "prettier": "prettier --write 'src/**/*.{js,jsx,scss,json}'"
  },
  "pre-commit": [
    "prettier"
  ],
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "get-graphql-schema": "^2.1.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.24.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.4",
    "jsdom": "^11.5.1",
    "lodash": "^4.17.10",
    "loglevel": "^1.6.0",
    "moment": "^2.20.1",
    "nock": "^9.1.5",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.9",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.1.3",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-icons": "^2.2.7",
    "react-infinite-scroller": "^1.1.2",
    "react-keydown": "^1.9.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "seamless-immutable": "^7.1.2",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.1",
    "superagent": "^3.8.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.4",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
